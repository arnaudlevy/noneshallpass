{
  "name": "None shall pass",
  "description": "Prevent unwanted websites to appear in your Google search results",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.google.com/*",
    "https://www.google.fr/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons" : {
    "48" : "icon-48.png",
    "96" : "icon-96.png"
  }
}
